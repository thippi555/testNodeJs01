{
  "manifestVersion": 1,
  "id": "sdk-event-validator-2025",
  "name": "SDK Event Validator",
  "version": "1.3.29",
  "publisher": "gyomusasshin",
  "description": "Azure DevOps SDK Event Validation Tool - NEW",
  "targets": [
    { "id": "Microsoft.VisualStudio.Services" }
  ],
  "categories": ["Azure Boards"],
  "icons": { "default": "icon.png" },
  "files": [
    { "path": "dist", "addressable": true },
    { "path": "icon.png", "addressable": true }
  ],
  "contributions": [
    {
      "id": "sdk-event-hub-new",
      "type": "ms.vss-web.hub",
      "targets": ["ms.vss-work-web.work-hub-group"],
      "properties": {
        "name": "SDK Event Validator",
        "uri": "dist/index.html"
      }
    },
    {
       "id": "sdk-event-onloaded",
       "type": "ms.vss-work-web.work-item-form-page",
       "targets": ["ms.vss-work-web.work-item-form"],
       "properties": {
       "name": "OnLoaded テスト",
       "uri": "https://4.190.217.82:3000/onloaded",
       "supportedWorkItemTypes": [ "Bug", "Task", "User Story", "Issue" ],
       "order": 100
      }
    },
    {
       "id": "sdk-event-onfieldchanged",
       "type": "ms.vss-work-web.work-item-form-page",
       "targets": ["ms.vss-work-web.work-item-form"],
       "properties": {
       "name": "OnFieldChanged テスト",
       "uri": "https://4.190.217.82:3000/onfieldchanged",
       "supportedWorkItemTypes": ["Bug", "Task", "User Story", "Issue"],
       "order": 101
      }
    },
    {
      "id": "sdk-event-onsaved",
      "type": "ms.vss-work-web.work-item-form-page",
      "targets": ["ms.vss-work-web.work-item-form"],
      "properties": {
      "name": "OnSaved テスト",
      "uri": "https://4.190.217.82:3000/onSaved",
      "supportedWorkItemTypes": ["Bug", "Task", "User Story", "Issue"],
      "order": 102
      }
    },
    {
      "id": "sdk-event-onreset",
      "type": "ms.vss-work-web.work-item-form-page",
      "targets": ["ms.vss-work-web.work-item-form"],
      "properties": {
      "name": "OnReset テスト",
      "uri": "https://4.190.217.82:3000/onReset",
      "supportedWorkItemTypes": ["Bug", "Task", "User Story", "Issue"],
      "order": 103
    }
  }

  ],
  "scopes": [
    "vso.work"
  ]
}
